# ======================================
# Studio Revenue Manager - Development Environment
# ======================================
# This file is used automatically when running: npm run dev
# Copy values from .env.example and customize for local development
# ======================================

# API Configuration
VITE_API_URL=http://localhost:3000
VITE_API_TIMEOUT=30000

# Application Configuration
VITE_APP_NAME=Studio Revenue Manager
VITE_APP_VERSION=0.1.0
VITE_APP_ENV=development

# Feature Flags (all enabled for development)
VITE_FEATURE_CALENDAR=true
VITE_FEATURE_CSV_UPLOAD=true
VITE_FEATURE_DASHBOARD=true
VITE_FEATURE_REPORTS=true
VITE_FEATURE_DARK_MODE=true

# Firebase Configuration (local emulator or development project)
# IMPORTANT: Replace with your actual Firebase development project credentials
VITE_FIREBASE_PROJECT_ID=studio-revenue-manager-dev
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=studio-revenue-manager-dev.firebaseapp.com
VITE_FIREBASE_STORAGE_BUCKET=studio-revenue-manager-dev.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# UI/UX Configuration
VITE_DEFAULT_LANGUAGE=ko
VITE_DEFAULT_CURRENCY=KRW
VITE_DEFAULT_TIMEZONE=Asia/Seoul
VITE_ITEMS_PER_PAGE=20
VITE_CHART_THEME=light

# Performance & Optimization
VITE_ENABLE_SW=false
VITE_API_RETRY_COUNT=3
VITE_DEBUG_MODE=true
VITE_ENABLE_PERFORMANCE=true

# Development Tools
VITE_USE_MOCK_API=false
VITE_SHOW_DEV_TOOLS=true

# Contact & Support
VITE_SUPPORT_EMAIL=dev@studiomorph.com
VITE_HELP_URL=http://localhost:5173/help
VITE_TERMS_URL=http://localhost:5173/terms
VITE_PRIVACY_URL=http://localhost:5173/privacy
